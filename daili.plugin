[General]
# > 跳过代理
# 跳过某个域名或者 IP 段，这些目标主机将不会由 Loon Proxy 处理。
skip-proxy = LEGIONR9000P, 192.168.31.0/32

# > Always Real IP Hosts
# DNS 数据包将被转发到上游 DNS 服务器。
real-ip = *.lan

[MitM]
# 不可MitM也不应该被打断连接的域名
hostname = 
